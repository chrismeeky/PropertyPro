{
  "name": "api",
  "version": "1.0.0",
  "description": "PropertyPro-lite enpoint to be consumed by my frontend",
  "main": "src/app.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --no-timeout --require @babel/register --require @babel/polyfill Server/test/*.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "nodemon --exec babel-node Server/index.js",
    "build": "babel Server --out-dir ./build --copy-files",
    "deploy": "gh-pages -d UI"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanio/PropertyPro-lite.git"
  },
  "author": "Aniefiok Akpan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zanio/PropertyPro-lite/issues"
  },
  "homepage": "https://github.com/zanio/PropertyPro-lite#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "datauri": "^2.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-router": "0.0.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1",
    "nyc": "^14.1.1",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
